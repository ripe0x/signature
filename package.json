    "preview": "vite preview",
    "deploy": "node scripts/deploy.js",
    "deploy:mainnet": "node scripts/deploy.js --network=mainnet",
    "deploy:fork": "node scripts/deploy.js --network=fork",
    "generate-outputs": "node scripts/generate-outputs.js",
    "generate-outputs:fork": "node scripts/generate-outputs.js --network=fork",
    "generate-outputs:mainnet": "node scripts/generate-outputs.js --network=mainnet",
    "use-historical-burns": "node scripts/use-historical-burns.js --network=fork",
