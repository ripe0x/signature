  "dependencies": {
    "dotenv": "^16.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.33.0",
    "twitter-api-v2": "^1.28.0",
    "viem": "^2.0.0"
